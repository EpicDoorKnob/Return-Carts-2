
<h1>All Products</h1>

<%= will_paginate %>

<table >
  <tr>
    <th>SKU</th>
    <th>UPC</th>
    <th>Description</th>
    <th>Customer</th>
    <th>Updated</th>
  </tr>


<% @products.each do |p| %>
  <tr>
    <td><%= p.sku %></td>
    <td><%= p.upc %></td>
    <td><%= p.description %></td>
    <td><%= p.customer %></td>
    <td><%= p.created_at %></td>
  </tr>
<% end %>
</table>

<%= will_paginate %>